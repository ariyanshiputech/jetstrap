{
  "name": "ariyanshipu/jetstrap",
  "description": "A Laravel 8+ package to easily switch TailwindCSS resources generated by Laravel Jetstream to Bootstrap 4 or 5.",
  "keywords": ["laravel", "bootstrap", "jetstream"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/ariyanshiputech/jetstrap/issues",
    "source": "https://github.com/ariyanshiputech/jetstrap"
  },
  "authors": [
    {
      "name": "Ariyan Shipu",
      "email": "noc@ariyanshipu.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/support": ">=8.0"
  },
  "autoload": {
    "psr-4": {
      "AriyanShipu\\Jetstrap\\": "src/AriyanShipu/Jetstrap/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AriyanShipu\\Jetstrap\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AriyanShipu\\Jetstrap\\JetstrapServiceProvider"
      ],
      "aliases": {
        "Jetstrap": "AriyanShipu\\Jetstrap\\JetstrapFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "tests": "vendor/bin/phpunit",
    "testsVerbose": "@tests -vvv"
  }
}
